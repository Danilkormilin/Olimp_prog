int main()
{
	int L, N, M,ts,te;
	scanf("%d%d%d", &L, &N, &M);
	for (int i = 0; i < N; i++)
	{
		scanf("%d%d", &ts, &te);
		for (int j = ts; j <= te; j++) a[j]++;
	}
	for (int i = 0; i < M; i++)
	{
		scanf("%d", &ts);
		printf("%d\n", a[ts]);
	}
	return (0);
}
