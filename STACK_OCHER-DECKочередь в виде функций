#include <stdio.h>
#define SIZE  1000
typedef struct queue
{
    int first, size;
    char data[SIZE];
} queue;
void initialise(queue * q)
{
    q -> size = 0;
    q -> first = 0;
}
void new(queue * q, int x)
{
    q -> data[(q -> first + q -> size) % SIZE] = x;
    q -> size++;
}
void drop(queue *q)
{
    q -> first++;
    if ( q-> first == SIZE) q -> first = 0;
    q -> size--;
}
char first(queue *q)
{
    return q -> data[q -> first];
}
int empty(queue * q)
{
    if (q -> size == 0) return 1;
    else return 0;
}
