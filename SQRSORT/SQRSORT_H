#include<stdio.h>
#include<math.h>

int main()
{
    int A[2000], n;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &A[i]);
    }
    for (int j = 1; j < n; j++)
    {
        int k = A[j];
        for (int i = j; i >= 0; i--)
        {
            if (i == 0)
            {
                A[i] = k;
                for (int h = 0; h < n; h++)
                {
                    printf("%d ", A[h]);
                }
                printf("\n");
                break;
            }
            if (k < A[i - 1]) A[i] = A[i - 1];
            else
            {
                A[i] = k;
                if (i != j)
                {
                    for (int h = 0; h < n; h++)
                    {
                        printf("%d ", A[h]);
                    }
                    printf("\n");
                }
                break;
            }
        }
    }
    return (0);
}
