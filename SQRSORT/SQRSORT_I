#include<stdio.h>
#include<math.h>

int main()
{
    int A[2000],B[2000], n;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d %d", &B[i], &A[i]);
    }
    for (int j = 1; j < n; j++)
    {
        int k = A[j];
        int g = B[j];
        for (int i = j; i >= 0; i--)
        {
            if (i == 0)
            {
                A[i] = k;
                B[i] = g;
                break;
            }
            if (k > A[i - 1])
            {
                A[i] = A[i - 1];
                B[i] = B[i - 1];
            }
            else if (k == A[i - 1] && g < B[i - 1])
            {
                A[i] = A[i - 1];
                B[i] = B[i - 1];
            }
            else
            {
                A[i] = k;
                B[i] = g;
                break;
            }
        }
    }
    for (int i = 0; i < n; i++)
    {
        printf("%d %d\n", B[i], A[i]);
    }
    return (0);
}
