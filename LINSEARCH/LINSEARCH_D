#include<stdio.h>
#include<math.h>

int main()
{
    int B[1001] = { 0 }, vx, n;
    scanf("%d", &n);
    for (int i = 1; i <= n; i++)
    {
        scanf("%d", &vx);
        B[i] = vx;
    }
    scanf("%d", &vx);
    for (int i = 1; i <= n; i++)
    {
        if (B[i] == vx) printf("%d ", i);
    }
    return (0);
}
