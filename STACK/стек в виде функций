#include <iostream>

 using namespace std;

typedef struct Stack
{
    int last;
    int data[1000];
} Stack;

int stack_top(Stack* s)
{
    return ((s)->data[s->last]);
}

void stack_init(Stack* s)
{
    s->last = -1;
}

void stack_pop(Stack* s)
{
    s->last--;
}

void stack_push(Stack* s, char n)
{
    s->data[++s->last] = n;
}

int stack_isempty(Stack* s)
{
    if (s->last == -1)
    {
        return 1;
    }
    else
    {
        return 0;
    }
}
